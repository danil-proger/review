#include <bits/stdc++.h>

int main() {
    int N, M;
    std::cin >> N;
    std::vector<int> array_first(N), array_second(M);
    for(int i = 0; i < N; ++i) {
        std::cin >> array_first[i];
    }
    std::cin >> M;
    std::vector<std::vector<int>> dp(N + 1, std::vector<int>(M + 1));
    for(int i = 0; i < M; ++i){
        std::cin >> array_second[i];
    }
    for(int i = 1; i < N + 1; ++i) {
        for(int j = 1; j < M + 1; ++j) {
            if (array_first[i - 1] != array_second[j - 1]) {
                dp[i][j] = std::max(dp[i - 1][j], dp[i][j - 1]);
            }
            else {
                dp[i][j] = dp[i - 1][j - 1] + 1;
            }
        }
    }
    std::cout << dp[N][M];
}
